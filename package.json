{
  "name": "nedoto-client",
  "description": "Nedoto Typescript HTTP Client provide access to the Nedoto HTTP APIs",
  "keywords": [
    "nedoto",
    "remote-config",
    "remote config",
    "remoteconfig",
    "real time config"
  ],
  "license": "MIT",
  "author": "Nicola Saliu<hello@nedoto.com>",
  "version": "1.0.3",
  "main": "./dist/cjs/NedotoClient.js",
  "module": "./dist/esm/NedotoClient.js",
  "types": "./dist/cjs/NedotoClient.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "watch": "webpack --mode development --watch",
    "build": "webpack --mode production",
    "lint": "eslint src tests --ext .ts",
    "lint-fix": "eslint src tests --fix --ext .ts",
    "format-check": "prettier -c src tests",
    "format": "prettier --write src tests",
    "test": "jest",
    "tsc": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "prepublishOnly": "npm run tsc"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.2",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "prettier": "^3.3.1",
    "terser-webpack-plugin": "^5.3.10",
    "ts-jest": "^29.1.4",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  }
}
